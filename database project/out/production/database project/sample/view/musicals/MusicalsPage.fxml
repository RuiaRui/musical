<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="448.0" prefWidth="781.0" styleClass="page7" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.view.musicals.MusicalsPageController">
   <children>
      <TableView fx:id="musicalTable" layoutX="-1.0" layoutY="37.0" prefHeight="344.0" prefWidth="781.0">
        <columns>
            <TableColumn fx:id="musicalColumn" prefWidth="123.0" text="Musical" />
            <TableColumn fx:id="dateColumn" prefWidth="48.0" text="Date" />
            <TableColumn fx:id="venueColumn" prefWidth="100.0" text="Venue" />
            <TableColumn fx:id="bookColumn" prefWidth="109.0" text="Book" />
            <TableColumn fx:id="notesColumn" prefWidth="122.0" text="Notes" />
            <TableColumn fx:id="referenceColumn" prefWidth="196.0" text="Reference" />
        </columns>
      </TableView>
      <Button layoutX="14.0" layoutY="389.0" mnemonicParsing="false" onAction="#handleLike" prefHeight="36.0" prefWidth="90.0" text="Like" />
      <Button layoutX="113.0" layoutY="389.0" mnemonicParsing="false" onAction="#handleShowDetails" prefHeight="36.0" prefWidth="112.0" text="Details" />
      <Label fx:id="label_wrong" layoutX="43.0" layoutY="428.0" textFill="#ee1111" />
      <Label fx:id="lblClose" layoutX="761.0" layoutY="3.0" onMouseClicked="#handleExit" onMousePressed="#handleExit" text="X" textFill="WHITE">
         <font>
            <Font name="System Bold" size="16.0" />
         </font>
      </Label>
      <ImageView fitHeight="22.0" fitWidth="24.0" layoutX="690.0" layoutY="3.0" onMousePressed="#handleMe" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../img/toys.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="27.0" fitWidth="24.0" layoutX="722.0" layoutY="2.0" onMousePressed="#handleHelp" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../img/trust.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="22.0" fitWidth="24.0" layoutX="3.0" layoutY="3.0" onMousePressed="#handleBack" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../img/back.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="22.0" fitWidth="24.0" layoutX="650.0" layoutY="3.0" onMousePressed="#handleCut" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../img/save.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
